func type_examples() {
    // Static typing
    let x: i32 = 42;
    let y: f64 = 3.14;

    // Dynamic typing
    let dyn message = "Dynamic typing";
    let dyn number = 100;

    // Type coercion
    let sum = x + number; // Results in i32

    // Collection with generic type
    let @numbers: Vec<i32> = [1, 2, 3, 4, 5];
    push(@numbers, 6);
}
