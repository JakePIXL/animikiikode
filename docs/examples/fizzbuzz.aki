func fizzbuzz(limit: i32) {
    let i: i32 = 1;
    while i <= limit {
        if i % 15 == 0 {
            println("FizzBuzz");
        } else if i % 3 == 0 {
            println("Fizz");
        } else if i % 5 == 0 {
            println("Buzz");
        } else {
            println(to_string(i));
        }
        i++
    }
}

func main() {
    fizzbuzz(100);
}
